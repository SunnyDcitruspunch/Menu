
@{
    ViewBag.Title = "Index";
}

<!DOCTYPE html>
<html>
<head>
    <script type="text/javascript" src="./lib/jquery-3.3.1.min.js"></script>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <link href="~/Content/bootstrap.min (2).css" rel="stylesheet" />
    <link href="~/Content/myStyle.css" rel="stylesheet" />
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div id="sidenav" class="btn-group-vertical" data-toggle="buttons">
        <a href="@Url.Action("AddOne", "Stack")" class="btn btn-primary" type="button" role="button">Add one time to Stack</a>
        <a href="@Url.Action("AddAlot", "Stack")" class="btn btn-primary" type="button" role="button">Add Huge List of Items to Stack</a>
        <a href="@Url.Action("Index", "Stack")" class="btn btn-primary" type="button" role="button">Display Stack</a>
        <a href="@Url.Action("DeleteAll", "Stack")" class="btn btn-primary" type="button" role="button">Delete from Stack</a>
        <a href="@Url.Action("Index", "Stack")" class="btn btn-primary" type="button" role="button">Clear Stack</a>
        <a href="@Url.Action("Index", "Stack")" class="btn btn-primary" type="button" role="button">Search Stack</a>
        <a href="@Url.Action("Index", "Home")" class="btn btn-primary" type="button" role="button">Return to Main Menu</a>
    </div>
    <div id="newentry" class="center">
        @foreach (var sData in ViewBag.MyStack)
        {
            <span>@sData &nbsp;</span>
        }
    </div>   
</body>
</html>

